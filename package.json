        "postcss-loader": "^8.1.1",
        "prettier": "^3.2.5",
        "sass": "^1.72.0",
        "sass-loader": "^16.0.2",
        "style-loader": "^3.3.4",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
